{
  "name": "jsx-info",
  "version": "1.6.1",
  "description": "displays a report of JSX component and prop usage",
  "main": "dist/api.js",
  "bin": "dist/jsx-info.js",
  "scripts": {
    "preversion": "npm test && rm -rf dist && tsc",
    "postversion": "git push && git push --tags && npm publish",
    "start": "tsc --watch",
    "upgrade": "npm-check --update",
    "test": "eslint src --ext .js,.ts && prettier --list-different \"src/**/*.{js,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wavebeem/jsx-info.git"
  },
  "keywords": [
    "jsx",
    "react",
    "stats",
    "usage",
    "statistics",
    "metrics"
  ],
  "author": "Brian Mock <brian@wavebeem.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.4.3",
    "@babel/traverse": "^7.4.3",
    "@types/babel__traverse": "^7.0.15",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "cosmiconfig": "^5.2.1",
    "globby": "^9.2.0",
    "ora": "^3.4.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "npm-check": "^5.9.0",
    "prettier": "^1.17.0",
    "typescript": "^4.0.5"
  }
}
